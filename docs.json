{
	"$schema": "https://mintlify.com/docs.json",
	"theme": "palm",
	"name": "Tembo Docs",
	"colors": {
		"primary": "#DF3021",
		"light": "#FFFFFF",
		"dark": "#000000"
	},
	"background": {
		"color": {
			"light": "#F8F8F8",
			"dark": "#080808"
		}
	},
	"favicon": "/favicon.ico",
	"api": {
		"openapi": "/openapi.documented.yml"
	},
	"navigation": {
		"tabs": [
			{
				"tab": "Documentation",
				"icon": "book",
				"groups": [
					{
						"group": "Get Started",
						"pages": ["welcome", "prompting-guide"]
					},
					{
						"group": "Features",
						"pages": [
							"features/coding-agents",
							"features/automations",
							"features/sandbox-environment",
							"features/mcp-servers",
							"features/pull-requests",
							"features/feedback-loop",
							"features/rule-files",
							"features/hooks"
						]
					},
					{
						"group": "Integrations",
						"pages": [
							"integrations/github",
							"integrations/gitlab",
							"integrations/bitbucket",
							"integrations/slack",
							"integrations/sentry",
							"integrations/linear",
							"integrations/jira",
							"integrations/postgres",
							"integrations/aws",
							"integrations/supabase",
							"integrations/raycast"
						]
					},
					{
						"group": "Admin",
						"pages": ["admin/security", "admin/billing", "admin/inviting-your-team"]
					},
					{
						"group": "Resources",
						"pages": ["resources/aifsd-manifesto", "resources/pwa", "changelog"]
					}
				]
			},
			{
				"tab": "Automation Examples",
				"icon": "robot",
				"groups": [
					{
						"group": "Automation Examples",
						"pages": [
							"automation-examples/overview",
							"automation-examples/common-patterns",
							"automation-examples/advanced-tips"
						]
					},
					{
						"group": "Examples by Category",
						"pages": [
							"automation-examples/product-changelog",
							"automation-examples/code-quality-cicd",
							"automation-examples/team-communication",
							"automation-examples/content-documentation"
						]
					}
				]
			},
			{
				"tab": "API Reference",
				"icon": "code",
				"openapi": "/openapi.documented.yml"
			}
		]
	},
	"logo": {
		"light": "/logo/light.svg",
		"dark": "/logo/dark.svg"
	},
	"navbar": {
		"links": [
			{
				"label": "Get Help",
				"href": "mailto:support@tembo.io"
			},
			{
				"label": "Book a Demo",
				"href": "https://book.avoma.com/tembo/tembo-demo/"
			}
		],
		"primary": {
			"type": "button",
			"label": "Dashboard",
			"href": "https://app.tembo.io"
		}
	},
	"footer": {
		"socials": {
			"x": "https://x.com/tembo_io"
		}
	},
	"redirects": [
		{
			"source": "/ai-agent/:slug*",
			"destination": "/features/:slug*"
		},
		{
			"source": "/ai-agent/tembo-md",
			"destination": "/features/rule-files"
		},
		{
			"source": "/docs/getting-started/postgres_guides/:slug*",
			"destination": "/integrations/postgres"
		}
	]
}
